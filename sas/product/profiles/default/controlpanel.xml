<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="sas.product">

    <configlet
        title="share a sale product configlet"
        action_id="sas.product.sasconfiglet"
        appId="sas.product"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@sasProductControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
